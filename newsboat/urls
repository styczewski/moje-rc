# -------- Query Feeds {{{
# ------------------------------------------------------
# http://www.newsbeuter.org/doc/newsbeuter.html#_query_feeds
# query:<name of feed>:<filter expression> [<tag> ...]
"query:All Unread Articles:unread = \"yes\""
"query:All Articles JW.ORG:tags # \"jw-org\"" "jw-org"
"query:All Articles Techniczne:tags # \"Techniczne\"" "Techniczne"
"query:All Articles Bezpieczenstwo:tags # \"Bezpieczenstwo\"" "Bezpieczenstwo"

"query:All Articles Wiadomosci:tags # \"Wiadomosci\"" "Wiadomosci"
"query:Oflagowane s - ciekawe:flags # \"s\"" "Oflagowane"
"query:Oflagowane b - bezpieczenstwo:flags # \"b\"" "Oflagowane"
"query:Oflagowane r - do przeczytania:flags # \"r\"" "Oflagowane"
# }}}
#

